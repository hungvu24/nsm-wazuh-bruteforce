wazuh_manager = monitor:.bash_history : CONTAINS : ./wazuh-install.sh -a -p 8080
manager_status = monitor:.bash_history : CONTAINS : systemctl status wazuh-manager
wazuh_agent = agent:.bash_history : CONTAINS : https://packages.wazuh.com/4.x/apt/pool/main/w/wazuh-agent/wazuh-agent
wazuh_ip = agent:/var/ossec/etc/ossec.conf : CONTAINS : 172.10.9.2
_hide = agent:.bash_history : CONTAINS : sudo apt install -y --fix-missing distro-info-data lsb-release
hydra_attack = attacker:.bash_history : CONTAINS : hydra -l root -P pass.txt
alerting = monitor:cat.stdout : CONTAINS : 5763
